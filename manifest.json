{
  "name": "Example 0: Hello World in Console",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This is an example to print Hello World in Chrome console",
  "permissions": ["storage", "alarms", "unlimitedStorage", "tabs"],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Sample",
    "default_icon": {
      "16": "icons/connpass.png",
      "48": "icons/connpass.png",
      "128": "icons/connpass.png"
    }
  },
  "icons": {
    "16": "icons/connpass.png",
    "48": "icons/connpass.png",
    "128": "icons/connpass.png"
  }
}
